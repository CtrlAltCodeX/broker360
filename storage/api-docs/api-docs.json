                                    },
                                    "number": {
                                        "type": "string",
                                        "example": "987654321"
                                    },
                                    "country": {
                                        "type": "integer",
                                        "example": 12
                                    },
                                    "identity": {
                                        "type": "integer",
                                        "example": 12
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
